Group:
  connection: 0
  tableName: groups
  columns:
    id:
      type: integer(4)
      fixed: 0
      unsigned: false
      primary: true
      autoincrement: true
    name:
      type: string(50)
      fixed: 0
      unsigned: false
      primary: false
      notnull: true
      autoincrement: false
    chief_id:
      type: integer(4)
      fixed: 0
      unsigned: false
      primary: false
      autoincrement: false
  relations:
    Users:
      class: User
      local: id
      foreign: group_id
      type: many
    Chief:
      class: User
      local: chief_id
      foreign: id
      type: one
User:
