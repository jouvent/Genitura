      unsigned: false
      primary: false
      notnull: true
      autoincrement: false
    tel2:
      type: string(20)
      fixed: 0
      unsigned: false
      primary: false
      notnull: true
      autoincrement: false
    type:
      type: string(1)
      fixed: 1
      unsigned: false
