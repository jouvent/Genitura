      type: integer(1)
      fixed: 0
      unsigned: false
      primary: false
      default: '0'
      notnull: true
      autoincrement: false
    is_vip:
      type: integer(1)
      fixed: 0
      unsigned: false
      primary: false
      default: '0'
      notnull: true
      autoincrement: false
    is_blacklisted:
      type: integer(1)
      fixed: 0
      unsigned: false
