    fk_paypal:
      type: integer(4)
      fixed: 0
      unsigned: false
      primary: false
      autoincrement: false
    group_id:
      type: integer(4)
      fixed: 0
      unsigned: false
      primary: false
      autoincrement: false
    bus_chief_id:
      type: integer(4)
      fixed: 0
